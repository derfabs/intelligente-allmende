    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "stream",
            "type": "python",
            "request": "launch",
            "program": "stream.py",
            "args": [
                "--network=models/ia_final.pkl",
                "--window_dimensions=1024,1024",
                "--serial_port=/dev/ttyACM0",
                "--baudrate=115200",
                "--min_speed=0.0",
                "--max_speed=0.5"
            ],
            "console": "integratedTerminal"
        }
    ]
}